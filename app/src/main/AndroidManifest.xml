<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.beibeilab.edgetoedgesample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EdgeToEdgeActivity"
            android:theme="@style/AppTheme.NoStatus.NoNavigation" />
        <activity
            android:name=".NoStatusBarActivity1"
            android:theme="@style/AppTheme.NoStatus" />
        <activity
            android:name=".NoStatusBarActivity2"
            android:theme="@style/AppTheme.NoStatus" />
        <activity
            android:name=".NoStatusBarActivity3"
            android:theme="@style/AppTheme.NoStatus" />
        <activity
            android:name=".NoStatusBarActivity4"
            android:theme="@style/AppTheme.NoStatus" />
    </application>

</manifest>